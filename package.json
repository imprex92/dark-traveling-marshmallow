{
  "name": "dark-traveling-marshmello",
  "version": "0.1.0",
  "main": "functions.js",
  "private": true,
  "browser": {
    "child_process": false
  },
  "scripts": {
    "dev-server": "node server.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-server": "NODE_ENV=production node server.js",
    "deploy": "yarn build && cross-env NODE_ENV=production firebase deploy --only functions,hosting",
    "SASS-Watch-M": "sass --watch ./styles/sass/materialize.scss:./styles/materialize.css",
    "SASS-Watch-SignIn": "sass --watch ./styles/sass/signIn.style.sass:./styles/signIn.style.css",
    "SASS-Watch-dashboard": "sass --watch ./styles/sass/dashboard.style.sass:./styles/dashboard.style.css",
    "SASS-watch-blogPost-page-component": "sass --watch ./styles/sass/blogPost-page.style.sass:./styles/blogPost-page.style.css",
    "SASS-watch-sidenav": "sass --watch ./styles/sass/sideNav.style.sass:./styles/sideNav.style.css",
    "SASS-watch-singlepost": "sass --watch ./styles/sass/singlePost.style.sass:./styles/singlePost.style.css",
    "SASS-watch-weather": "sass --watch ./styles/sass/weather.style.sass:./styles/weather.style.css"
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@materializecss/materialize": "^1.1.0",
    "@splidejs/react-splide": "^0.7.12",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "country-list": "^2.2.0",
    "dotenv-webpack": "^6.0.0",
    "express": "^4.18.2",
    "firebase": "^8.2.0",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^3.22.0",
    "moment": "^2.29.4",
    "next": "^13.1.5",
    "react": "^18.2.0",
    "react-cool-onclickoutside": "^1.5.9",
    "react-dom": "^18.2.0",
    "react-typewriter-hook": "^1.0.1",
    "react-world-flags": "^1.5.1",
    "sass": "^1.32.0",
    "sharp": "^0.30.5",
    "typewriter-effect": "^2.17.0",
    "use-places-autocomplete": "^1.8.1",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/materialize-css": "^1.0.8",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "resolve-url-loader": "^5.0.0",
    "webpack-stats-plugin": "^1.1.1"
  }
}
